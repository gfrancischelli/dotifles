const keyframes = {
    fade_in: [
        { opacity: 0, transform: 'translateY(30px'},
        { opacity: 1, transform: 'translateY(0)'}
    ],
}



const timings = {
    default: {
        duration: 700,
        easing: 'ease-out',
        fill: 'forwards',
    }
}


const defaults = { keyframes: keyframes, timings: timings }

module.exports = defaults;
